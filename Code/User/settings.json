{
  "editor.cursorBlinking": "expand",
  "workbench.colorTheme": "One Dark Pro Darker",
  "editor.guides.bracketPairs": true,
  "editor.fontFamily": "'FantasqueSansM Nerd Font', monospace",
  "editor.fontLigatures": true,
  "terminal.integrated.fontFamily": "'FantasqueSansM Nerd Font', monospace",
  "workbench.productIconTheme": "fluent-icons",
  "symbols.hidesExplorerArrows": false,
  "workbench.iconTheme": "symbols",
  "oneDarkPro.bold": true,
  "oneDarkPro.vivid": true,
  "editor.cursorSmoothCaretAnimation": "on",
  "explorer.confirmDelete": false,
  "workbench.sideBar.location": "right",
  "workbench.preferredLightColorTheme": "Default Dark+",
  "editor.fontSize": 13,
  "editor.wordWrap": "on",
  "editor.tabSize": 2,
  "editor.linkedEditing": true,
  "editor.codeActionsOnSave": {
      "source.fixAll.eslint": "always"
  },
  "workbench.startupEditor": "none",
  "redhat.telemetry.enabled": false,
  "git.openRepositoryInParentFolders": "never",
  "markdown.preview.scrollPreviewWithEditor": false,
  "markdown.preview.scrollEditorWithPreview": false,
  "pasteImage.prefix": "image(\"images/",
  "pasteImage.directoryName": "images",
  "pasteImage.suffix": "\")"
}